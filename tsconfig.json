{
    "compilerOptions": {
        "resolveJsonModule": true,
        "target": "ES5",
        "module": "commonjs",
        "downlevelIteration": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "outDir": "./dist",
        "lib": ["es2015", "es2016", "es2017", "es2018", "es2019", "es2020"],
        "skipLibCheck": true
    }
}
